{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": false,
    "noImplicitReturns": true,
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*"
  ]
}
